
# Makefile changes for SUN's running SUNOS
# ARCH = SUN5

CC     = gcc
CXX    = CC
F77    = f77
AR     = ar
RANLIB = ranlib

# the following load flags works for the siesta machine,
# and should be changed for different SUN machines
MISC_LIB = -L/Net/local/gnu/lib -lg++ -lF77 -lV77 -lM77 -lsunmath

